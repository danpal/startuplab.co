%header.header
  %h1.logo
    =link_to image_tag("logo.png", :tiltle=> "startuplab.co", :alt => "startuplab.co"), index_path, :class => "logo"
  %nav.utility
    %ul
      %li= link_to "Blog", '/blog'
      %li= link_to "Events", events_path
      %li= link_to "Rules",  rules_path
      %li= link_to "Photos", #
      %li= link_to "Companies", companies_path
      - if current_user.nil?
        %li= link_to "Register/Sign in", signin_path
      -else
        -if current_user.admin?
          %li= link_to "Admin", admin_path
        %li= link_to "Sign out", signout_path

