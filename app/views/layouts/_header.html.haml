%header.header
  %h1.logo
    =link_to image_tag("logo.png", :tiltle=> "startuplab.co", :alt => "startuplab.co"), index_path, :class => "logo"
    %em.tag_line 
      Colombian Startups Unleashed
  %nav.utility
    %ul
      %li= link_to "Blog", #
      %li= link_to "Events", #
      %li= link_to "About", #
      %li= link_to "Photos", #
      %li= link_to "Companies", #
      - if current_user.nil?
        %li= link_to "Register/Sign in", signin_path
      -else
        -if current_user.admin?
          %li= link_to "Admin", admin_path
        %li= link_to "Sign out", signout_path

